<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.abc.myproject.user.vo.UserModel" table="tbl_user" lazy="false">
    
    <id name="uuid">
		<generator class="assigned"/>
	</id>
	
	<property name="name" />
	<property name="age" />
	
	
</class>
</hibernate-mapping>